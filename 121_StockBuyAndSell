class Solution {
    public int maxProfit(int[] prices) {
        int min = prices[0];
        int cost =0;
        int profit =0;
        for(int price:prices){
            cost = price - min;
            profit = Math.max(profit, cost); //storing the profit , which is max
            min = Math.min(price, min); //calculating the minimum value from 1 to i-1, where i is the profit

        }
        return profit;
    }
}
